	email = kino.ma.ms@gmail.com
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[core]
	excludesfile = ~/.gitignore_global
[pull]
	ff = only
[alias]
	c = commit -m
	s = status
	d = diff
	ds = diff --staged
